<!DOCTYPE html>
<html lang="{{ app.locale }}" data-bs-theme="{{ Interface.Databstheme }}" dir="{{ Interface.Dir }}">
    <head>
        <meta charset="UTF-8">
        <meta name="robots" content="noindex, nofollow">
		<meta name="autor" content="Kataev Yaroslav Georgievich">
		<meta name="viewport" content="width=device-width, initial-scale=1">
        <title>Katyargeor Framework for Business</title>
        <link rel="icon" sizes="48X48" href="{{ asset('favicon.ico') }}">
        <link rel="icon" sizes="32X32" href="{{ asset('favicon/favicon-32x32.png') }}" type="image/png">
		<link rel="icon" sizes="256X256" href="{{ asset('favicon/favicon-256x256.png') }}" type="image/png">
		<link rel="icon" sizes="512X512" href="{{ asset('favicon/favicon-512x512.png') }}" type="image/png">
		<link rel="icon" size="1024X1024" href="{{ asset('favicon/favicon-1024x1024.png') }}" type="image/png">
		<link rel="icon" sizes="any" href="{{ asset('favicon/favicon.svg') }}" type="image/svg+xml">
        <link rel="stylesheet" href="{{ asset('jquery-ui/themes/base/base.css') }}">
        <link rel="stylesheet" href="{{ asset('jquery-ui/dist/themes/' ~ Interface.Theme ~ '/theme.css') }}">
        <link rel="stylesheet" href="{{ asset('jquery-ui/dist/themes/' ~ Interface.Theme ~ '/jquery-ui.min.css') }}">
        <link rel="stylesheet" href="{{ asset('bootstrap/css/bootstrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset('css/button.css') }}">
        {% block stylesheets %}{% endblock %}
        {{ importmap('app') }}
        <script src="{{ asset('jquery/dist/jquery.min.js') }}"></script>
        <script src="{{ asset('jquery-ui/dist/jquery-ui.min.js') }}"></script>
        <script src="{{ asset('jquery-ui/ui/version.js') }}"></script>
        <script src="{{ asset('jquery-ui/ui/jquery-patch.js') }}"></script>
        <script src="{{ asset('jquery-ui/ui/plugin.js') }}"></script>
        <script src="{{ asset('jquery-ui/ui/widget.js') }}"></script>
        <script src="{{ asset('jquery-ui/ui/unique-id.js') }}"></script>
        <script src="{{ asset('jquery-ui/ui/data.js') }}"></script>
        <script src="{{ asset('jquery-ui/ui/disable-selection.js') }}"></script>
        <script src="{{ asset('jquery-ui/ui/effect.js') }}"></script>
        <script src="{{ asset('jquery-ui/ui/focusable.js') }}"></script>
        <script src="{{ asset('jquery-ui/ui/form.js') }}"></script>
        <script src="{{ asset('jquery-ui/ui/form-reset-mixin.js') }}"></script>
        <script src="{{ asset('jquery-ui/ui/ie.js') }}"></script>
        <script src="{{ asset('jquery-ui/ui/jquery-var-for-color.js') }}"></script>
        <script src="{{ asset('jquery-ui/ui/keycode.js') }}"></script>
        <script src="{{ asset('jquery-ui/ui/labels.js') }}"></script>
        <script src="{{ asset('jquery-ui/ui/position.js') }}"></script>
        <script src="{{ asset('jquery-ui/ui/safe-active-element.js') }}"></script>
        <script src="{{ asset('jquery-ui/ui/safe-blur.js') }}"></script>
        <script src="{{ asset('jquery-ui/ui/scroll-parent.js') }}"></script>
        <script src="{{ asset('jquery-ui/ui/tabbable.js') }}"></script>
        <script src="{{ asset('jquery-ui/ui/i18n/datepicker-' ~ Interface.Datepicker ~ '.js') }}"></script>
        <script src="{{ asset('jquery-ui/ui/effects/effect-' ~ Interface.Effect ~ '.js') }}"></script>
        <script src="{{ asset('bootstrap/js/bootstrap.bundle.min.js') }}"></script>
        <script src="{{ asset('js/color-modes.js') }}"></script>
        {% block javascripts %}{% endblock %}
    </head>
    <body>
        {% include 'button.svg.twig' %}
        {% block body %}{% endblock %}
    </body>
</html>
